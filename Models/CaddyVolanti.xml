<PropertyList>

  <path>CaddyVolanti.ac</path>

  <nasal>
    <load>
      var livery_update = aircraft.livery_update.new("Aircraft/CaddyVolanti/Models/Liveries");
      var self = cmdarg();

      var float_0 = self.getNode("sim/multiplay/generic/float[0]", 1);
      var float_1 = self.getNode("sim/multiplay/generic/float[1]", 1);

    </load>
  <unload><![CDATA[
    livery_update.stop();
  ]]></unload>
  </nasal>
  <!-- Main rotor-->
 <animation>
    <type>spin</type>
<object-name>Main Rotor</object-name>
 <property>rotors/main/rpm</property>
